{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Django Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver"],
            "django": true,
            "noDebug": true
        },
        {
            "name": "Debug Django Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver"],
            "django": true
        }
    ]
}
