{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver"],
            "django": true,
            "noDebug": true,
            "env": {"DJANGO_DEV_RELOAD": "1"}
        },
        {
            "name": "Debug Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver"],
            "django": true,
            "env": {"DJANGO_DEV_RELOAD": "1"}
        }
    ]
}
